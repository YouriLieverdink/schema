{
  "name": "common",
  "description": "Shared models between multiple applications.",

  "models": {
    "error": {
      "description": "A generic error message.",
      "fields": [
        { "name": "code", "type": "string" },
        { "name": "message", "type": "string" }
      ]
    },

    "healthcheck": {
      "description": "Wraps the status and version of a service.",
      "fields": [
        { "name": "status", "type": "string" },
        { "name": "version", "type": "string" }
      ]
    }
  }
}
