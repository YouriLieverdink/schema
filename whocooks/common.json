{
  "name": "whocooks-common",
  "description": "Common enums and models between the services of whocooks.",

  "models": {
    "error": {
      "description": "Generic error message.",
      "fields": [
        { "name": "code", "type": "string" },
        { "name": "message", "type": "string" }
      ]
    },

    "validation_error": {
      "description": "Validation error message.",
      "fields": [
        { "name": "field", "type": "string" },
        { "name": "errors", "type": "[string]" }
      ]
    },

    "healthcheck": {
      "description": "The status of a service in a given moment.",
      "fields": [{ "name": "status", "type": "string" }]
    }
  }
}
